    <head>
        <title>Mi Página OCHO</title> 
        
    </head>
    <body>    
   
        <h1> Mi 8ª web de fibonacci</h1>
          <script>
            document.write ("<br>");
              
            var numero =  prompt("Introduzca un número entre 1 y 100:");
            numero = Number(numero);
            while (numero < 1 || numero > 100)
            {
       numero = prompt("Número inválido. Introduzca un número entre 1 y 100:");
       numero = Number (numero);
            }
                     
            var a = 0; 
            var b = 1;

        
            for (i = 1; i <= numero; i++) {
                c = a + b;
                     document.write ("<br>La serie de fibonacci "+c);
                a = b;
                b = c;
       
            }

          </script>
</body>
